{
  "name": "SYBPush",
  "version": "0.1.0",
  "summary": "百度Push服务SDK",
  "description": "百度Push服务SDK",
  "homepage": "https://github.com/isandboy/SYBPush",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "isandboy": "sandboylu@gmail.com"
  },
  "source": {
    "git": "https://github.com/isandboy/SYBPush.git",
    "tag": "0.1.0"
  },
  "platforms": {
    "ios": "9.0"
  },
  "default_subspecs": "normal",
  "subspecs": [
    {
      "name": "normal",
      "source_files": [
        "SYBPush/Classes/*.{h,m}",
        "SYBPush/Classes/normal/*.{h}"
      ],
      "vendored_libraries": "SYBPush/Classes/normal/*.a",
      "pod_target_xcconfig": {
        "OTHER_LDFLAGS": "-all_load"
      },
      "frameworks": [
        "Foundation",
        "CoreTelephony",
        "SystemConfiguration",
        "CoreLocation"
      ],
      "libraries": "z"
    },
    {
      "name": "idfa",
      "source_files": [
        "SYBPush/Classes/*.{h,m}",
        "SYBPush/Classes/idfa/*.{h}"
      ],
      "vendored_libraries": "SYBPush/Classes/idfa/*.a",
      "pod_target_xcconfig": {
        "OTHER_LDFLAGS": "-all_load"
      },
      "frameworks": [
        "Foundation",
        "CoreTelephony",
        "SystemConfiguration",
        "CoreLocation",
        "AdSupport"
      ],
      "libraries": "z"
    }
  ]
}
