{
  "name": "SYBPush",
  "version": "1.6.2",
  "summary": "百度Push服务SDK",
  "description": "百度Push服务SDK 版本：2020-06-02 iOS V1.6.2",
  "homepage": "https://github.com/isandboy/SYBPush",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "isandboy": "sandboylu@gmail.com"
  },
  "source": {
    "git": "https://github.com/isandboy/SYBPush.git",
    "tag": "1.6.2"
  },
  "platforms": {
    "ios": "9.0"
  },
  "default_subspecs": "normal",
  "subspecs": [
    {
      "name": "normal",
      "source_files": [
        "SYBPush/Classes/*.{h,m}",
        "SYBPush/Classes/normal/*.{h}"
      ],
      "vendored_libraries": "SYBPush/Classes/normal/*.a",
      "frameworks": [
        "Foundation",
        "CoreTelephony",
        "SystemConfiguration",
        "CoreLocation"
      ],
      "libraries": "z",
      "pod_target_xcconfig": {
        "OTHER_LDFLAGS": "-all_load",
        "VALID_ARCHS": "x86_64 armv7 arm64"
      }
    },
    {
      "name": "idfa",
      "source_files": [
        "SYBPush/Classes/*.{h,m}",
        "SYBPush/Classes/idfa/*.{h}"
      ],
      "vendored_libraries": "SYBPush/Classes/idfa/*.a",
      "frameworks": [
        "Foundation",
        "CoreTelephony",
        "SystemConfiguration",
        "CoreLocation",
        "AdSupport"
      ],
      "libraries": "z",
      "pod_target_xcconfig": {
        "OTHER_LDFLAGS": "-all_load",
        "VALID_ARCHS": "x86_64 armv7 arm64"
      }
    }
  ]
}
